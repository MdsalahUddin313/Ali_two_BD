<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_centerInParent="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <RelativeLayout
        android:layout_width="match_parent"
        android:id="@+id/p_layout_one_id"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="12dp"
        android:gravity="center"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/p_cardview_one_id"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="15dp"
            android:orientation="vertical"
            >
            <ImageButton
                android:id="@+id/p_button_one_id"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:background="@drawable/beauty"
                >

            </ImageButton>
        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/p_cardview_two_id"

            android:layout_marginRight="15dp"
            android:layout_toRightOf="@+id/p_cardview_one_id"
            android:orientation="vertical"
            >
            <ImageButton
                android:id="@+id/p_button_two_id"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:background="@drawable/travel"
                >
            </ImageButton>


        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:orientation="vertical"
            android:layout_marginRight="15dp"
            android:id="@+id/p_cardview_three_id"
            android:layout_toRightOf="@+id/p_cardview_two_id"
            >
            <ImageButton
                android:id="@+id/p_button_three_id"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:background="@drawable/toys"
                >
            </ImageButton>
        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="10dp"
            android:id="@+id/p_cardview_four_id"
            android:layout_marginRight="15dp"
            android:layout_toRightOf="@+id/p_cardview_three_id"
            >
            <ImageButton
                android:id="@+id/p_button_four_id"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:background="@drawable/technology"
                >
            </ImageButton>
        </LinearLayout>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:id="@+id/p_layout_two_id"
        android:layout_below="@+id/p_layout_one_id"
        android:layout_marginRight="5dp"
        android:layout_marginLeft="10dp"
        android:gravity="center"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/p_cardview_six_id"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="15dp"
            android:orientation="vertical"
            >
            <ImageButton
                android:id="@+id/p_button_six_id"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:background="@drawable/fashion"
                >
            </ImageButton>
        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/p_cardview_seven_id"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/p_cardview_six_id"
            android:orientation="vertical"
            android:layout_marginRight="15dp"
            >
            <ImageButton
                android:id="@+id/p_button_seven_id"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:background="@drawable/watch"
                >
            </ImageButton>
        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:orientation="vertical"
            android:id="@+id/p_cardview_eight_id"
            android:layout_toRightOf="@+id/p_cardview_seven_id"
            android:layout_marginRight="15dp"
            >
            <ImageButton
                android:id="@+id/p_button_eight_id"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:background="@drawable/bags"
                >
            </ImageButton>
        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="10dp"
            android:id="@+id/p_cardview_nine_id"
            android:layout_toRightOf="@+id/p_cardview_eight_id"
            android:layout_marginRight="15dp"
            >
            <ImageButton
                android:id="@+id/p_button_nine_id"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:background="@drawable/hdphone"
                >
            </ImageButton>
        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>